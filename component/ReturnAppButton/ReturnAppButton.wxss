.return-button-wrapper {
    position: fixed;
    top: 240rpx;
    left: 20rpx;
    display: flex;
    align-items: center;
    height: 56rpx;
    background-color: rgba(255,255,255,0.9);
    border-radius: 34rpx;
    z-index: 10;
}

.return-button {
    display: flex;
    align-items: center;
    padding: 0!important;
    margin: 0!important;
    border: none!important;
    background: none!important;
}

.return-button::after {
    padding: 0!important;
    margin: 0!important;
    border: none!important;
    background: none!important;
}

.return-icon {
    margin-left: 8rpx;
    height: 32rpx;
    width: 32rpx;
    background: url("data:image/png;base64,") no-repeat center center;
    background-size: 100%;
}

.return-text {
    padding: 0 22rpx 0 16rpx;
    font-size: 24rpx;
    height: 28rpx;
    line-height: 28rpx;
    color: #000000;
    border-right: 1px solid #ddd;
}

.close-icon {
    width: 57rpx;
    height: 56rpx;
    background: url("data:image/png;base64,") no-repeat center center;
    background-size: 20rpx;
}