@import "../citylist/citylist.wxss";

.register-container {
    height: 100%;
    width: 100%;
    flex-direction: column;
    display: flex;
    background: #efeff4;
    font-family: "微软雅黑","华文细黑",Arial,Helvetica,sans-serif;
}

.rg-section-1 {
    height: 76rpx;
    font-size: 26rpx;
    padding: 20rpx 0;
    width: 100%;
    background-color: #efeff4;
    color: #808080;
    text-align: center;
    line-height: 76rpx;
}

.rg-section-2 {
    padding: 18rpx 30rpx 0 30rpx;
}

.rg-section-box {
    border: 1rpx solid #e0dede;
    height: 98rpx;
    display: flex;
    margin-top: 12rpx;
    border-radius: 15rpx;
    background-color: #fff;
    align-items: center;
    box-shadow: 2rpx 2rpx 10rpx #e8e8e8,2rpx 2rpx 10rpx #e8e8e8;
}

.rg-section-icon1 {
    width: 96rpx;
    height: 100%;
    background: url("https://imgorder.kfc.com.cn/mwos/wechatAppImages/icon-user.png") no-repeat center center;
    background-size: 28rpx;
    border-right: 1rpx solid #e0e0e0;
}

.rg-section-icon2 {
    width: 96rpx;
    height: 100%;
    background: url("https://imgorder.kfc.com.cn/mwos/wechatAppImages/icon-phone.png") no-repeat center center;
    background-size: 28rpx 40rpx;
    border-right: 1rpx solid #e0e0e0;
}

.rg-section-icon3 {
    width: 96rpx;
    height: 100%;
    background: url("https://imgorder.kfc.com.cn/mwos/wechatAppImages/icon-code.png") no-repeat center center;
    background-size: 28rpx;
    border-right: 1rpx solid #e0e0e0;
}

.rg-section-icon4 {
    width: 96rpx;
    height: 100%;
    background: url("https://imgorder.kfc.com.cn/mwos/wechatAppImages/icon-city.png") no-repeat center center;
    background-size: 28rpx;
    border-right: 1rpx solid #e0e0e0;
}

input {
    padding: 0 10rpx;
    font-size: 30rpx;
    color: #bfbfbf;
}

.rg-section-t1 {
    color: #808080;
    margin: 0 10px;
    font-size: 28rpx;
    flex: 1;
    width: 100%;
    height: 100%;
    display: flex;
    align-items: center;
}

.rg-section-v1 {
    flex: 1;
    margin-right: 10rpx;
    border: 1rpx solid #e0dede;
    border-radius: 10rpx;
    line-height: 80rpx;
    font-size: 28rpx;
    color: #808080;
    padding-left: 30rpx;
}

.rg-section-2-check {
    display: flex;
    height: 40rpx;
    color: #a5a5a5;
    font-size: 28rpx;
}

.rg-section-icon5 {
    width: 96rpx;
    height: 100%;
    background: url("https://imgorder.kfc.com.cn/mwos/wechatAppImages/icon-check-on.png") no-repeat center center;
    background-size: 48rpx 38rpx;
}

.rg-section-icon5-un {
    background: url("https://imgorder.kfc.com.cn/mwos/wechatAppImages/icon-check-un.png") no-repeat center center;
    background-size: 48rpx 48rpx;
}

.rg-section-2-bt {
    height: 100rpx;
    background-color: #d93035;
    margin-top: 58rpx;
    font-size: 38rpx;
    color: #fff;
    border-radius: 15rpx;
    line-height: 100rpx;
    text-align: center;
    margin-bottom: 33rpx;
}

.rg-section-v2 {
    color: #004be4;
}

.rg-section-v3 {
    border: 1rpx solid #d6d4d4;
    background-color: #fff;
    border-radius: 64rpx;
