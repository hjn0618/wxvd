    height: 40rpx;
    line-height: 40rpx;
    text-align: center;
    margin-left: 28rpx;
}

.rg-section-v4 {
    margin-left: 70rpx;
}

.rg-section-box2 {
    display: flex;
}

.rg-section-box3 {
    display: flex;
    width: 360rpx;
}

.rg-section-ipt1 {
    width: 360rpx;
}

.rg-section-on {
    background: #d93035;
    color: #fff;
    border: 1px solid #FFF;
}

.rg-section-v5 {
    border: 1px solid #bfbfbf;
    font-size: 22rpx;
    height: 58rpx;
    line-height: 58rpx;
    width: 138rpx;
    text-align: center;
    margin-right: 28rpx;
    border-radius: 10rpx;
    color: #d93035;
}

.rg-section-ipt3 {
    flex: 1;
}

.rg-section-v6 {
    color: #939393;
}

.see-no {
    height: 58rpx;
    width: 50rpx;
    background: url("https://imgorder.kfc.com.cn/mwos/wechatAppImages/icon-passwordNotSee.png") no-repeat right center;
    background-size: 45rpx 23rpx;
    margin-right: 28rpx;
}

.see {
    height: 58rpx;
    width: 50rpx;
    background: url("https://imgorder.kfc.com.cn/mwos/wechatAppImages/icon-passwordSee.png") no-repeat right center;
    background-size: 45rpx 23rpx;
    margin-right: 28rpx;
}

.passwordinput {
    flex: 1;
}

.rg-1 {
    height: 114rpx;
    line-height: 114rpx;
    background: url(https://imgorder.kfc.com.cn/mwos/wechatAppImages/icon-popup-top.png) no-repeat top center;
    background-size: 100rpx 20rpx;
    font-size: 36rpx;
    color: #000;
    border-radius: 15rpx 15rpx 0 0;
    font-weight: bold;
    border-bottom: 1px solid #e6e3e3;
}

.rg-1-1 {
    width: 80rpx;
    height: 80rpx;
    background: url("https://imgorder.kfc.com.cn/mwos/wechatAppImages/icon-close.png") no-repeat center center;
    background-size: 40rpx 38rpx;
    position: absolute;
    top: 14rpx;
    left: 10rpx;
}

.rg-2 {
    text-align: left;
    font-size: 28rpx;
    padding: 50rpx;
}

.rg-t2 {
    color: #a5a5a5;
    line-height: 50rpx;
}

.rg-3-1 {
    flex: 1;
    height: 100rpx;
    line-height: 100rpx;
    background-color: #d93035;
    color: #fff;
    border: 1px solid #ccc;
    border-radius: 10rpx;
    font-size: 36rpx;
    font-weight: 700;
}

.rg-3 {
    padding: 30rpx 50rpx;
}

.rg-container {
