page {
    display: flex;
    flex-direction: column;
    height: 100%;
    width: 100%;
}

/* 折扣信息区 */
.discount {
    width: 100%;
    height: 50rpx;
    line-height: 50rpx;
    background: #fef9e6;
    font-size: 28rpx;
    text-align: center;
    color: #999;
}

.discount-txt {
    color: #fff;
    padding: 5rpx 5rpx;
    background: red;
    margin: auto;
}

.content {
    flex: 1;
    display: flex;
}

/* 左侧菜单 */

.category {
    width: 202rpx;
    height: 100%;
    background: #fcfcfc;
    font-size: 28rpx;
}

/* 隐藏滚动条 */

::-webkit-scrollbar {
    width: 0;
    height: 0;
    color: transparent;
}

.category-item {
    height: 100rpx;
    line-height: 100rpx;
    text-align: center;
}

.category-unselect {
    color: #6c6c6c;
    background: #f9f9f9;
    border-bottom: 1rpx solid #e3e3e3;
}

.category-selected {
    color: #ff9c35;
    border-left: 6rpx solid #ff9c35;
    background: white;
}

/* 右侧菜单 */

.food-category {
    font-size: 24rpx;
    background: #f3f4f6;
    padding: 10rpx;
    color: #ff9c35;
}

.food-item {
    display: flex;
    margin: 40rpx 20rpx;
}

.food-item-pic {
    margin-right: 20rpx;
    width: 94rpx;
    height: 94rpx;
}

.food-item-pic>image {
    width: 100%;
    height: 100%;
}

.food-item-info {
    flex: 1;
    font-size: 30rpx;
    margin-top: 4rpx;
}

.food-item-price {
    margin-top: 14rpx;
    color: #f05a86;
}

.food-item-opt {
    margin-top: 40rpx;
}

.food-item-opt>i:before {
    font-size: 44rpx;
    color: #ff9c35;
    content: "\e728";
}

/* 满减区域 */

.promotion {
    height: 40rpx;
    background: #ff9b9b;
    color: #fff7ec;
    font-size: 24rpx;
    text-align: center;
}

/* 底部菜单操作 */

.operate {
    height: 110rpx;
    display: flex;
    box-shadow: 0 -12rpx 24rpx 0 rgba(0, 0, 0, 0.08);
}

.operate-full{
    padding-bottom: 30rpx;
}

.operate-shopcart {
    display: flex;
    width: 74%;
    padding: 10rpx;
    background: #FFFFFF !important;

}

.operate-submit {
    display: flex;
    flex-direction: column;
    width: 35%;

    font-size: 30rpx;
    background: rgb(221, 221, 221);
    color: #aaa;
    line-height: 33rpx;
    text-align: center;
    align-items: center;
    justify-content: center;
    margin: 8px !important;
    justify-content: center !important;
    border-radius: 58rpx !important;
}

.operate-submit-activity {
    background: #D62F35 !important;
    color: #fff;
}


.operate-shopcart-icon {
    width: 80rpx;
    height: 80rpx;
    padding-left: 36rpx;
    background: url(https://imgorder.kfc.com.cn/mwos/Version/icon-shopping-red.png) no-repeat center center;
    background-size: 80rpx 80rpx;
    position: relative;

}

.operate-shopcart-icon-activity {
    background-size: 80rpx 80rpx;
}


.operate-shopcart-icon>span {
    padding: 2rpx 14rpx;
    border-radius: 30%;
    background: #D62F35;
    color: white;
    font-size: 22rpx;
    position: absolute;
    text-align: center;
    top: 0px;
    right: -10rpx;
}

.operate-shopcart-ball {
    width: 36rpx;
    height: 36rpx;
    position: fixed;
    border-radius: 50%;
    left: 50%;
    top: 50%;
    background: #D62F35;
}

.operate-shopcart-empty {
    color: #a9a9a9;
    line-height: 88rpx;
    font-size: 30rpx;
}

.operate-shopcart-price {
    display: flex;
}

.operate-shopcart-price>view {
    font-size: 40rpx;
    line-height: 88rpx;
    margin-left: 35rpx;
    color: #D62F35;
}

.operate-shopcart-price>text {
    font-size: 24rpx;
    line-height: 92rpx;
    margin-left: 15rpx;
    color: #aaa;
    text-decoration: line-through;
}

.shopcart {
    position: fixed;
    top: 0;
    left: 0;
    right: 0;
    font-size: 28rpx;
}

.shopcart-mask {
    position: absolute;
    top: 0;
    right: 0;
    bottom: 0;
    left: 0;
    background: #000;
    opacity: 0.5;
}

.shopcart-wrap {
    position: absolute;
    width: 100%;
    max-height: 90%;
    bottom: 0;
    background: #fff;
    overflow: scroll;
}

.shopcart-head {
    position: fixed;
    width: 100%;
    background: #f0f0f0;
    color: #878787;
    line-height: 80rpx;
    font-size: 26rpx;
    overflow: hidden;
}

.shopcart-head-title {
    display: flex;
    align-items:center;
    float: left;
    margin-left: 25rpx;

}

.shopcart-head-title:before {
    background: #D62F35;
    width: 8rpx;
    height: 32rpx;
    content: "";
    display: inline-block;
    margin-right: 10rpx;
    position: relative;
}


.shopcart-head-clean {
    float: right;
    margin-right: 25rpx;
    background: url(http://localhost:8080/wxxx/images/trash.png) no-repeat center left;
    padding-left: 40rpx;
    background-size: 35rpx 35rpx;
}

.shopcart-list {
    margin-top: 101rpx;
}

.shopcart-item {
    display: flex;
    padding: 20rpx 20rpx;
    line-height: 40rpx;
    align-items:center;
}

.shopcart-item>view {
    margin-left: 30rpx;
}

.shopcart-item:not(:last-child) {
    border-bottom: 1rpx solid #e3e3e3;
}

.shopcart-item-name {
    flex: 1;
}

.shopcart-item-price {
    color: #D62F35;
}

.shopcart-item-number {
    display: flex;
}

.shopcart-item-number>view {
